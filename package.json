{
  "name": "cjpollok-gatsby-base",
  "description": "A base Gatsby setup for use in other projects",
  "version": "1.1.0",
  "author": "Chris Pollok (cjpollok)",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "jest": "jest",
    "lint": "eslint src && stylelint src/**/*.scss",
    "test": "npm run lint && npm run jest"
  },
  "dependencies": {
    "gatsby": "2.13.70",
    "gatsby-plugin-manifest": "2.2.5",
    "gatsby-plugin-offline": "2.2.6",
    "gatsby-plugin-react-helmet": "3.1.3",
    "gatsby-plugin-sass": "2.1.10",
    "node-sass": "4.12.0",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-helmet": "5.2.1"
  },
  "devDependencies": {
    "babel-eslint": "10.0.2",
    "babel-jest": "23.6.0",
    "babel-preset-gatsby": "0.2.10",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "6.2.1",
    "eslint-config-airbnb": "18.0.1",
    "eslint-config-prettier": "6.1.0",
    "eslint-plugin-import": "2.18.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.0",
    "eslint-plugin-react": "7.14.3",
    "identity-obj-proxy": "3.0.0",
    "jest": "^23.4.2",
    "prettier": "1.18.2",
    "stylelint": "10.1.0",
    "stylelint-config-standard": "18.3.0"
  },
  "jest": {
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "moduleFileExtensions": [
      "js",
      "scss"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|ico)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "testURL": "http://localhost/",
    "transform": {
      ".js": "<rootDir>/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjpollok/gatsby-base"
  }
}
