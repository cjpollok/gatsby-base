{
  "name": "cjpollok-gatsby-base",
  "description": "A base Gatsby setup for use in other projects",
  "version": "1.0.0",
  "author": "Chris Pollok (cjpollok)",
  "dependencies": {
    "gatsby": "2.0.45",
    "gatsby-plugin-manifest": "2.0.8",
    "gatsby-plugin-offline": "2.0.13",
    "gatsby-plugin-react-helmet": "3.0.1",
    "gatsby-plugin-sass": "2.0.4",
    "node-sass": "4.10.0",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "react-helmet": "5.2.0"
  },
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "jest": "jest",
    "lint": "eslint src && stylelint src/styles/**/*.scss",
    "test": "npm run lint && npm run jest"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "babel-preset-gatsby": "0.1.3",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.9.1",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "stylelint": "^9.4.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "jest": {
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "moduleFileExtensions": [
      "js",
      "scss"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "identity-obj-proxy",
      ".+\\.(jpg|jpeg|png|gif|ico)$": "<rootDir>/__mocks__/fileMock.js"
    },
    "setupFiles": [
      "<rootDir>/loadershim.js"
    ],
    "setupTestFrameworkScriptFile": "./test/setup.js",
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/.cache/"
    ],
    "testURL": "http://localhost/",
    "transform": {
      ".js": "<rootDir>/jest-preprocess.js"
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cjpollok/gatsby-base"
  }
}
